    <!-- http://www.w3.org/2003/01/geo/wgs84_pos#location -->

    <owl:ObjectProperty rdf:about="http://www.w3.org/2003/01/geo/wgs84_pos#location">
        <rdfs:range rdf:resource="http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing"/>
    </owl:ObjectProperty>



    <!-- http://www.wurvoc.org/vocabularies/om-1.8/singular_unit -->

    <owl:ObjectProperty rdf:about="http://www.wurvoc.org/vocabularies/om-1.8/singular_unit">
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">singular unit</rdfs:label>
    </owl:ObjectProperty>



    <!-- https://w3id.org/saref#IsUsedFor -->

    <owl:ObjectProperty rdf:about="https://w3id.org/saref#IsUsedFor">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship specifying the purpose for which a device is used for (e.g., controlling a Commodity, a Property or a BuildingObject)</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Is used for</rdfs:label>
    </owl:ObjectProperty>



    <!-- https://w3id.org/saref#accomplishes -->

    <owl:ObjectProperty rdf:about="https://w3id.org/saref#accomplishes">
        <owl:inverseOf rdf:resource="https://w3id.org/saref#isAccomplishedBy"/>
        <rdfs:range rdf:resource="https://w3id.org/saref#Task"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship between a certain entity (e.g., a device) and the task it accomplishes</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">accomplishes</rdfs:label>
    </owl:ObjectProperty>



    <!-- https://w3id.org/saref#actsUpon -->

    <owl:ObjectProperty rdf:about="https://w3id.org/saref#actsUpon">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship between a command and a state</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">acts upon</rdfs:label>
    </owl:ObjectProperty>



    <!-- https://w3id.org/saref#consistsOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/saref#consistsOf">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship indicating a composite entity that consists of other entities (e.g., a temperature/humidity sensor that consists of a temperature sensor and a humidity sensor)</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">consists of</rdfs:label>
    </owl:ObjectProperty>



    <!-- https://w3id.org/saref#contains -->

    <owl:ObjectProperty rdf:about="https://w3id.org/saref#contains">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship of containment between entities, for example between a building space and a device or building object.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">contains</rdfs:label>
    </owl:ObjectProperty>



    <!-- https://w3id.org/saref#hasCategory -->

    <owl:ObjectProperty rdf:about="https://w3id.org/saref#hasCategory">
        <owl:inverseOf rdf:resource="https://w3id.org/saref#isCategoryOf"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship between an entity (such as device or function) and a corresponding category</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has category</rdfs:label>
    </owl:ObjectProperty>



    <!-- https://w3id.org/saref#hasCommand -->

    <owl:ObjectProperty rdf:about="https://w3id.org/saref#hasCommand">
        <owl:inverseOf rdf:resource="https://w3id.org/saref#isCommandOf"/>
        <rdfs:range rdf:resource="https://w3id.org/saref#Command"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship between an entity (such as a function) and a command</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has command</rdfs:label>
    </owl:ObjectProperty>



    <!-- https://w3id.org/saref#hasConsumption -->

    <owl:ObjectProperty rdf:about="https://w3id.org/saref#hasConsumption">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship identifying the type of consumption of an entity</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has consumption</rdfs:label>
    </owl:ObjectProperty>



    <!-- https://w3id.org/saref#hasFunction -->

    <owl:ObjectProperty rdf:about="https://w3id.org/saref#hasFunction">
        <rdfs:range rdf:resource="https://w3id.org/saref#Function"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship identifying the type of function of a device</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has function</rdfs:label>
    </owl:ObjectProperty>



    <!-- https://w3id.org/saref#hasInputParameter -->

    <owl:ObjectProperty rdf:about="https://w3id.org/saref#hasInputParameter">
        <rdfs:domain rdf:resource="https://w3id.org/saref#Service"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship identifying the input parameter of a service</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has input parameter</rdfs:label>
    </owl:ObjectProperty>



    <!-- https://w3id.org/saref#hasMeterReadingTime -->

    <owl:ObjectProperty rdf:about="https://w3id.org/saref#hasMeterReadingTime">
        <rdfs:range rdf:resource="https://w3id.org/saref#Time"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship between a metering function and time identifying the reading time of a measurement</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has meter reading time</rdfs:label>
    </owl:ObjectProperty>



    <!-- https://w3id.org/saref#hasMeterReadingValue -->

    <owl:ObjectProperty rdf:about="https://w3id.org/saref#hasMeterReadingValue">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship between a metering function and a value identifying the reading value of a measurement</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has meter reading</rdfs:label>
    </owl:ObjectProperty>



    <!-- https://w3id.org/saref#hasOutputParameter -->

    <owl:ObjectProperty rdf:about="https://w3id.org/saref#hasOutputParameter">
        <rdfs:domain rdf:resource="https://w3id.org/saref#Service"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship identifying the output parameter of a service</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has output parameter</rdfs:label>
    </owl:ObjectProperty>



    <!-- https://w3id.org/saref#hasPrice -->

    <owl:ObjectProperty rdf:about="https://w3id.org/saref#hasPrice">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationships indentifying the price associated to an entity</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has price</rdfs:label>
    </owl:ObjectProperty>



    <!-- https://w3id.org/saref#hasProduction -->

    <owl:ObjectProperty rdf:about="https://w3id.org/saref#hasProduction">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship identifying the type of production of an entity</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has production</rdfs:label>
    </owl:ObjectProperty>



    <!-- https://w3id.org/saref#hasProfile -->

    <owl:ObjectProperty rdf:about="https://w3id.org/saref#hasProfile">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship associating a profile to a certain entity (e.g., a device)</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has profile</rdfs:label>
    </owl:ObjectProperty>



    <!-- https://w3id.org/saref#hasSensingRange -->

    <owl:ObjectProperty rdf:about="https://w3id.org/saref#hasSensingRange">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship between a sensing function and a property identifying the range of a sensor detection</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has sensing range </rdfs:label>
    </owl:ObjectProperty>



    <!-- https://w3id.org/saref#hasSensingTime -->

    <owl:ObjectProperty rdf:about="https://w3id.org/saref#hasSensingTime">
        <rdfs:range rdf:resource="https://w3id.org/saref#Time"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship between a sensing function and time identifying the time of a sensor detection</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has sensing time</rdfs:label>
    </owl:ObjectProperty>



    <!-- https://w3id.org/saref#hasSingularUnit -->

    <owl:ObjectProperty rdf:about="https://w3id.org/saref#hasSingularUnit">
        <rdfs:subPropertyOf rdf:resource="http://www.wurvoc.org/vocabularies/om-1.8/singular_unit"/>
        <rdfs:domain rdf:resource="https://w3id.org/saref#UnitOfMeasure"/>
        <rdfs:range rdf:resource="https://w3id.org/saref#UnitOfMeasure"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship defining a singular unit as a sub-property of the om:singular_unit property</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has singular unit</rdfs:label>
    </owl:ObjectProperty>



    <!-- https://w3id.org/saref#hasState -->

    <owl:ObjectProperty rdf:about="https://w3id.org/saref#hasState">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship identifying the type of state of a device</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has state</rdfs:label>
    </owl:ObjectProperty>



    <!-- https://w3id.org/saref#hasThreshold -->

    <owl:ObjectProperty rdf:about="https://w3id.org/saref#hasThreshold">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship associated with an event function to notify that a certain threshold value has been exceeded</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has threshold</rdfs:label>
    </owl:ObjectProperty>



    <!-- https://w3id.org/saref#hasTime -->

    <owl:ObjectProperty rdf:about="https://w3id.org/saref#hasTime">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship to associate time information to an entity</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has time</rdfs:label>
    </owl:ObjectProperty>



    <!-- https://w3id.org/saref#hasTypicalConsumption -->

    <owl:ObjectProperty rdf:about="https://w3id.org/saref#hasTypicalConsumption">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship identifying the typical (energy or power) consumption of a device</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">has typical consumption</rdfs:label>
    </owl:ObjectProperty>



    <!-- https://w3id.org/saref#isAccomplishedBy -->

    <owl:ObjectProperty rdf:about="https://w3id.org/saref#isAccomplishedBy">
        <rdfs:domain rdf:resource="https://w3id.org/saref#Task"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship indentifying the task accomplished by a certain entity (e.g., a device)</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">is accomplished by</rdfs:label>
    </owl:ObjectProperty>



    <!-- https://w3id.org/saref#isCategoryOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/saref#isCategoryOf">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship between a category and an entity (such as device or function)</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">is category of</rdfs:label>
    </owl:ObjectProperty>



    <!-- https://w3id.org/saref#isCommandOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/saref#isCommandOf">
        <rdfs:domain rdf:resource="https://w3id.org/saref#Command"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship between a command and a function.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">is command of</rdfs:label>
    </owl:ObjectProperty>



    <!-- https://w3id.org/saref#isLocatedIn -->

    <owl:ObjectProperty rdf:about="https://w3id.org/saref#isLocatedIn">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship between a device and the building space in which it is located. </rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">is located in</rdfs:label>
    </owl:ObjectProperty>



    <!-- https://w3id.org/saref#isMeasuredIn -->

    <owl:ObjectProperty rdf:about="https://w3id.org/saref#isMeasuredIn">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship identifying the unit of measure used for a certain entity.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">is measured in</rdfs:label>
    </owl:ObjectProperty>



    <!-- https://w3id.org/saref#isOfferedBy -->

    <owl:ObjectProperty rdf:about="https://w3id.org/saref#isOfferedBy">
        <owl:inverseOf rdf:resource="https://w3id.org/saref#offers"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship between a service and a device that offers the service</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">is offered by</rdfs:label>
    </owl:ObjectProperty>



    <!-- https://w3id.org/saref#offers -->

    <owl:ObjectProperty rdf:about="https://w3id.org/saref#offers">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship between a device and a service</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">offers</rdfs:label>
    </owl:ObjectProperty>



    <!-- https://w3id.org/saref#represents -->

    <owl:ObjectProperty rdf:about="https://w3id.org/saref#represents">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relationship between a service and a function.</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">represents</rdfs:label>
    </owl:ObjectProperty>



